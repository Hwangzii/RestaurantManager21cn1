<Window x:Class="loginPage.CrowWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:loginPage"
        mc:Ignorable="d"
        Title="CrowWindow" Height="450" Width="800" WindowState="Maximized">

    <Grid>
        <Button x:Name="crowBtn" Click="crowBtn_Click">
            <Image x:Name="crowImage" Source="/Images/Crow/Crow.jpg" Stretch="Fill">
            </Image>
        </Button>

        <TextBlock Text="Press [Crow] to Crow" FontSize="45" HorizontalAlignment="Center" VerticalAlignment="Top">
        </TextBlock>
    </Grid>
    
</Window>
